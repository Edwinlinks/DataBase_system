c;cACZoB,IAAI,EAAE,SADM;cAEZC,OAAO,EAAE;YAFG,CAAd;UAIF,CALA,MAKO;YACL,MAAI,CAACrB,QAAL,CAAc;cACZoB,IAAI,EAAE,OADM;cAEZC,OAAO,EAAEvB,GAAG,CAACwB;YAFD,CAAd;UAIF;;UAEA,MAAI,CAACvC,aAAL,GAAqB,KAArB,CAd+C,CAcnB;;UAC5B,MAAI,CAACS,IAAL,GAf+C,CAenC;;QACb,CAhBD;MAiBF;IAED,CA7EM;IA8EPgC,UA9EO,sBA8EI7B,GA9EJ,EA8ES;MACd,KAAKb,IAAL,GAAY2C,IAAI,CAACC,KAAL,CAAWD,IAAI,CAACE,SAAL,CAAehC,GAAf,CAAX,CAAZ;MACA,KAAKZ,aAAL,GAAqB,IAArB;IACD,CAjFM;IAkFP6C,YAlFO,wBAkFMC,EAlFN,EAkFU;MAAA;;MACf/D,OAAO,CAACgE,MAAR,CAAe,eAAeD,EAA9B,EAAkChC,IAAlC,CAAuC,aAAE,EAAK;QAC5C,IAAIC,GAAG,CAACC,IAAJ,KAAa,GAAjB,EAAsB;UACpB,MAAI,CAACC,QAAL,CAAc;YACZoB,IAAI,EAAE,SADM;YAEZC,OAAO,EAAE;UAFG,CAAd;QAIF,CALA,MAKO;UACL,MAAI,CAACrB,QAAL,CAAc;YACZoB,IAAI,EAAE,OADM;YAEZC,OAAO,EAAEvB,GAAG,CAACwB;UAFD,CAAd;QAIF;;QACA,MAAI,CAAC9B,IAAL,GAZ4C,CAY/B;;MACd,CAbD;IAcD,CAjGM;IAkGPuC,gBAlGO,4BAkGU5C,QAlGV,EAkGoB;MAAI;MAC7B,KAAKA,QAAL,GAAgBA,QAAhB;MACA,KAAKK,IAAL;IACD,CArGM;IAsGPwC,mBAtGO,+BAsGa1B,OAtGb,EAsGsB;MAAG;MAC9B,KAAKpB,WAAL,GAAmBoB,OAAnB;MACA,KAAKd,IAAL;IACF;EAzGO;AA3CI,CAAf","names":["request","name","components","data","pickerOptions","shortcuts","text","onClick","picker","$emit","Date","date","setTime","getTime","vueDate","loading","form","dialogVisible","bookVis","search","currentPage","pageSize","total","tableData","roles","created","load","methods","handleChange","row","put","then","res","code","$message","success","$router","push","get","params","pageNum","records","console","log","handleUploadSuccess","exportUser","location","href","window","server","filesUploadUrl","add","save","purchaseId","type","message","msg","post","handleEdit","JSON","parse","stringify","handleDelete","id","delete","handleSizeChange","handleCurrentChange"],"sourceRoot":"","sources":["/home/luotinkai/Documents/代码及数据库/fxBox/vue/src/views/Purchase.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n\n    <!--    功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-upload\n        action=\"http://localhost:9090/purchase/import\"\n        :on-success=\"handleUploadSuccess\"\n        :show-file-list=false\n        :limit=\"1\"\n        accept='.xlsx'\n        style=\"display: inline-block; margin: 0 10px\"\n      >\n        <el-button type=\"primary\">导入</el-button>\n      </el-upload>\n      <el-button type=\"primary\" @click=\"exportUser\">导出</el-button>\n    </div>\n\n    <!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"请输入产品名\" style=\"width: 20%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n    <el-table\n        v-loading=\"loading\"\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column\n          prop=\"purchaseId\"\n          label=\"采购单编号\"\n          sortable\n      >\n      </el-table-column>\n      <el-table-column\n          prop=\"productName\"\n          label=\"产品名\">\n      </el-table-column>\n      <el-table-column\n          prop=\"companyName\"\n          label=\"供应商\">\n      </el-table-column>\n      <el-table-column\n          prop=\"adminUsername\"\n          label=\"经手人\">\n      </el-table-column>\n      <el-table-column\n          prop=\"purchaseDate\"\n          label=\"采购时间\">\n      </el-table-column>\n      <el-table-column label=\"操作\" width=\"400\">\n        <template #default=\"scope\">\n          <el-button size=\"mini\" type=\"primary\" plain @click=\"handleEdit(scope.row)\">编辑</el-button>\n<!--          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.purchaseId)\">-->\n<!--            <template #reference>-->\n<!--              <el-button size=\"mini\" type=\"danger\">删除</el-button>-->\n<!--            </template>-->\n<!--          </el-popconfirm>-->\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n\n\n    <el-dialog title=\"提示\" v-model=\"dialogVisible\" width=\"30%\">\n      <el-form :model=\"form\" label-width=\"120px\">\n<!--        <el-form-item label=\"采购单号\">-->\n<!--          <el-input v-model=\"form.purchaseId\" style=\"width: 80%\"></el-input>-->\n<!--        </el-form-item>-->\n        <el-form-item label=\"产品名\">\n          <el-input v-model=\"form.productName\" style=\"width: 80%\" placeholder=\"必填\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"供应商\">\n          <el-input v-model=\"form.companyName\" style=\"width: 80%\" placeholder=\"必填\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"经手人\">\n          <el-input v-model=\"form.adminUsername\" style=\"width: 80%\" placeholder=\"经手人\"></el-input>\n        </el-form-item>\n        <el-form-item label=\"采购时间\">\n          <div class=\"block\">\n            <el-date-picker\n                v-model=\"form.vueDate\"\n                type=\"datetime\"\n                placeholder=\"选择日期时间\">\n            </el-date-picker>\n          </div>\n        </el-form-item>\n      </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'Outbox',\n  components: {},\n  data() {\n    return {\n      pickerOptions: {\n        shortcuts: [{\n          text: '今天',\n          onClick(picker) {\n            picker.$emit('pick', new Date());\n          }\n        }, {\n          text: '昨天',\n          onClick(picker) {\n            const date = new Date();\n            date.setTime(date.getTime() - 3600 * 1000 * 24);\n            picker.$emit('pick', date);\n          }\n        }, {\n          text: '一周前',\n          onClick(picker) {\n            const date = new Date();\n            date.setTime(date.getTime() - 3600 * 1000 * 24 * 7);\n            picker.$emit('pick', date);\n          }\n        }]\n      },\n      vueDate: '',\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      bookVis: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      roles: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    handleChange(row) {\n      request.put(\"/purchase/changeRole\", row).then(res => {\n        if (res.code === '0') {\n          this.$message.success(\"更新成功\")\n          if (res.data) {\n            this.$router.push(\"/login\")\n          }\n        }\n      })\n    },\n    load() {\n      this.loading = true\n      request.get(\"/purchase\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n        console.log(this.form)\n      })\n\n    },\n    handleUploadSuccess(res) {\n      if (res.code === \"0\") {\n        this.$message.success(\"导入成功\")\n        this.load()\n      }\n    },\n    exportUser() {\n      location.href = \"http://\" + window.server.filesUploadUrl + \":9090/purchase/export\";\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n    },\n    save() {\n      if (this.form.purchaseId) {  // 更新\n        request.put(\"/purchase\", this.form).then(res => {\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.dialogVisible = false  // 关闭弹窗\n          this.load() // 刷新表格的数据\n        })\n      } else {  // 新增\n        request.post(\"/purchase\", this.form).then(res => {\n          console.log(this.form)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.dialogVisible = false  // 关闭弹窗\n          this.load() // 刷新表格的数据\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      request.delete(\"/purchase/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.currentPage = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"]}]}