MAAI,CAACrB,QAAL,CAAc;YACZoB,IAAI,EAAE,OADM;YAEZC,OAAO,EAAEvB,GAAG,CAACwB;UAFD,CAAd;QAIF;;QACA,MAAI,CAAC9B,IAAL,GAZuC,CAY1B;;MACd,CAbD;IAcD,CAjGM;IAkGPuC,gBAlGO,4BAkGU5C,QAlGV,EAkGoB;MAAI;MAC7B,KAAKA,QAAL,GAAgBA,QAAhB;MACA,KAAKK,IAAL;IACD,CArGM;IAsGPwC,mBAtGO,+BAsGa1B,OAtGb,EAsGsB;MAAG;MAC9B,KAAKpB,WAAL,GAAmBoB,OAAnB;MACA,KAAKd,IAAL;IACF;EAzGO;AApBI,CAAf","names":["request","name","components","data","loading","form","dialogVisible","bookVis","search","currentPage","pageSize","total","tableData","roles","created","load","methods","handleChange","row","put","then","res","code","$message","success","$router","push","get","params","pageNum","records","console","log","handleUploadSuccess","exportUser","location","href","window","server","filesUploadUrl","add","save","boxId","type","message","msg","post","handleEdit","JSON","parse","stringify","handleDelete","id","delete","handleSizeChange","handleCurrentChange"],"sourceRoot":"","sources":["/home/luotinkai/Documents/代码及数据库/fxBox/vue/src/views/Box.vue"],"sourcesContent":["<template>\n  <div style=\"padding: 10px\">\n\n    <!--    功能区域-->\n    <div style=\"margin: 10px 0\">\n      <el-button type=\"primary\" @click=\"add\">新增</el-button>\n      <el-upload\n        action=\"http://localhost:9090/box/import\"\n        :on-success=\"handleUploadSuccess\"\n        :show-file-list=false\n        :limit=\"1\"\n        accept='.xlsx'\n        style=\"display: inline-block; margin: 0 10px\"\n      >\n        <el-button type=\"primary\">导入</el-button>\n      </el-upload>\n      <el-button type=\"primary\" @click=\"exportUser\">导出</el-button>\n    </div>\n\n    <!--    搜索区域-->\n    <div style=\"margin: 10px 0\">\n      <el-input v-model=\"search\" placeholder=\"请输入仓位物理地址\" style=\"width: 20%\" clearable></el-input>\n      <el-button type=\"primary\" style=\"margin-left: 5px\" @click=\"load\">查询</el-button>\n    </div>\n    <el-table\n        v-loading=\"loading\"\n        :data=\"tableData\"\n        border\n        stripe\n        style=\"width: 100%\">\n      <el-table-column\n          prop=\"boxId\"\n          label=\"仓位编号\"\n          sortable\n      >\n      </el-table-column>\n      <el-table-column\n          prop=\"boxAddr\"\n          label=\"仓位物理地址\">\n      </el-table-column>\n      <el-table-column\n          prop=\"productId\"\n          label=\"物资编号\">\n      </el-table-column>\n      <el-table-column\n          prop=\"productName\"\n          label=\"物资名称\">\n      </el-table-column>\n<!--      <el-table-column label=\"角色列表\" width=\"300\">-->\n<!--        <template #default=\"scope\">-->\n<!--          <el-select v-model=\"scope.row.roles\" multiple placeholder=\"请选择\" style=\"width: 80%\">-->\n<!--            <el-option v-for=\"item in roles\" :key=\"item.id\" :label=\"item.comment\" :value=\"item.id\"></el-option>-->\n<!--          </el-select>-->\n<!--        </template>-->\n<!--      </el-table-column>-->\n      <el-table-column label=\"操作\" width=\"400\">\n        <template #default=\"scope\">\n<!--          <el-button size=\"mini\" type=\"primary\" @click=\"handleChange(scope.row)\">保存角色信息</el-button>-->\n<!--          <el-button size=\"mini\" type=\"success\" plain @click=\"showBooks(scope.row.bookList)\">查看图书列表</el-button>-->\n          <el-button size=\"mini\" type=\"primary\" plain @click=\"handleEdit(scope.row)\">编辑</el-button>\n          <el-popconfirm title=\"确定删除吗？\" @confirm=\"handleDelete(scope.row.boxId)\">\n            <template #reference>\n              <el-button size=\"mini\" type=\"danger\">删除</el-button>\n            </template>\n          </el-popconfirm>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <div style=\"margin: 10px 0\">\n      <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"currentPage\"\n          :page-sizes=\"[5, 10, 20]\"\n          :page-size=\"pageSize\"\n          layout=\"total, sizes, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </div>\n\n\n    <el-dialog title=\"用户拥有的图书列表\" v-model=\"bookVis\" width=\"30%\">\n      <el-table :data=\"bookList\" stripe border>\n        <el-table-column prop=\"bid\" label=\"ID\"></el-table-column>\n        <el-table-column prop=\"name\" label=\"名称\"></el-table-column>\n        <el-table-column prop=\"price\" label=\"价格\"></el-table-column>\n      </el-table>\n    </el-dialog>\n\n    <el-dialog title=\"提示\" v-model=\"dialogVisible\" width=\"30%\">\n      <el-form :model=\"form\" label-width=\"120px\">\n<!--        <el-form-item label=\"仓位编号\">-->\n<!--          <el-input v-model=\"form.boxId\" style=\"width: 80%\"></el-input>-->\n<!--        </el-form-item>-->\n        <el-form-item label=\"仓位物理地址\">\n          <el-input v-model=\"form.boxAddr\" style=\"width: 80%\" placeholder=\"必填\"></el-input>\n        </el-form-item>\n<!--        <el-form-item label=\"物品编号\">-->\n<!--          <el-input v-model=\"form.productId\" style=\"width: 80%\"></el-input>-->\n<!--        </el-form-item>-->\n        <el-form-item label=\"物资名称\">\n          <el-input v-model=\"form.productName\" style=\"width: 80%\" placeholder=\"必填\"></el-input>\n        </el-form-item>\n      </el-form>\n      <template #footer>\n          <span class=\"dialog-footer\">\n            <el-button @click=\"dialogVisible = false\">取 消</el-button>\n            <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n          </span>\n\n      </template>\n    </el-dialog>\n\n  </div>\n</template>\n\n<!--    事件标签-->\n<script>\n\nimport request from \"@/utils/request\";\n\nexport default {\n  name: 'Inbox',\n  components: {},\n  data() {\n    return {\n      loading: true,\n      form: {},\n      dialogVisible: false,\n      bookVis: false,\n      search: '',\n      currentPage: 1,\n      pageSize: 10,\n      total: 0,\n      tableData: [],\n      roles: []\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    handleChange(row) {\n      request.put(\"/box/changeRole\", row).then(res => {\n        if (res.code === '0') {\n          this.$message.success(\"更新成功\")\n          if (res.data) {\n            this.$router.push(\"/login\")\n          }\n        }\n      })\n    },\n    load() {\n      this.loading = true\n      request.get(\"/box\", {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          search: this.search\n        }\n      }).then(res => {\n        this.loading = false\n        this.tableData = res.data.records\n        this.total = res.data.total\n        console.log(res.data)\n      })\n\n    },\n    handleUploadSuccess(res) {\n      if (res.code === \"0\") {\n        this.$message.success(\"导入成功\")\n        this.load()\n      }\n    },\n    exportUser() {\n      location.href = \"http://\" + window.server.filesUploadUrl + \":9090/box/export\";\n    },\n    add() {\n      this.dialogVisible = true\n      this.form = {}\n    },\n    save() {\n      if (this.form.boxId) {  // 更新\n        request.put(\"/box\", this.form).then(res => {\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"更新成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n          this.dialogVisible = false  // 关闭弹窗\n          this.load() // 刷新表格的数据\n        })\n      } else {  // 新增\n        request.post(\"/box\", this.form).then(res => {\n          console.log(this.form)\n          if (res.code === '0') {\n            this.$message({\n              type: \"success\",\n              message: \"新增成功\"\n            })\n          } else {\n            this.$message({\n              type: \"error\",\n              message: res.msg\n            })\n          }\n\n          this.dialogVisible = false  // 关闭弹窗\n          this.load() // 刷新表格的数据\n        })\n      }\n\n    },\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogVisible = true\n    },\n    handleDelete(id) {\n      request.delete(\"/box/\" + id).then(res => {\n        if (res.code === '0') {\n          this.$message({\n            type: \"success\",\n            message: \"删除成功\"\n          })\n        } else {\n          this.$message({\n            type: \"error\",\n            message: res.msg\n          })\n        }\n        this.load()  // 删除之后重新加载表格的数据\n      })\n    },\n    handleSizeChange(pageSize) {   // 改变当前每页的个数触发\n      this.pageSize = pageSize\n      this.load()\n    },\n    handleCurrentChange(pageNum) {  // 改变当前页码触发\n      this.currentPage = pageNum\n      this.load()\n    }\n  }\n}\n</script>\n"]}]}